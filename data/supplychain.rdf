        <rdfs:range rdf:Resource="Employee" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="sellsTo">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Customer" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="usesEquipment">
        <rdfs:domain rdf:Resource="Plant" />
        <rdfs:range rdf:Resource="Equipment" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="transportedBy">
        <rdfs:domain rdf:Resource="Shipment" />
        <rdfs:range rdf:Resource="TransportationMode" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="conducts">
        <rdfs:domain rdf:Resource="Partner" />
        <rdfs:range rdf:Resource="FinancialTransaction" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="conformsTo">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="ComplianceStandard" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="adheresTo">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="EnvironmentalStandard" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="meets">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="SafetyStandard" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="classifiedAs">
        <rdfs:domain rdf:Resource="SupplyChainEvent" />
        <rdfs:range rdf:Resource="SupplyChainEventType" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="mitigates">
        <rdfs:domain rdf:Resource="Office" />
        <rdfs:range rdf:Resource="RiskManagement" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="reports">
