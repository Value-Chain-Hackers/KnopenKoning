    <owl:Class rdf:ID="Supplier" />
    <owl:Class rdf:ID="Office" />
    <owl:Class rdf:ID="Plant" />
    <owl:Class rdf:ID="Warehouse" />
    <owl:Class rdf:ID="Brand" />
    <owl:Class rdf:ID="ProductLine" />
    <owl:Class rdf:ID="Product" />
    <owl:Class rdf:ID="Distributor" />
    <owl:Class rdf:ID="Partner" />
    <owl:Class rdf:ID="Store" />
    <owl:Class rdf:ID="SupplyChainEvent" />
    <owl:Class rdf:ID="RawMaterial" />
    <owl:Class rdf:ID="Inventory" />
    <owl:Class rdf:ID="Order" />
    <owl:Class rdf:ID="Shipment" />
    <owl:Class rdf:ID="Delivery" />
    <owl:Class rdf:ID="Payment" />
    <owl:Class rdf:ID="Transportation" />
    <owl:Class rdf:ID="Logistics" />
    <owl:Class rdf:ID="QualityControl" />
    <owl:Class rdf:ID="SafetyCompliance" />
    <owl:Class rdf:ID="EnvironmentalImpact" />
    <owl:Class rdf:ID="ProductionSchedule" />
    <owl:Class rdf:ID="Machine" />
    <owl:Class rdf:ID="ProductionLine" />
    <owl:Class rdf:ID="Operator" />
    <owl:Class rdf:ID="MaintenanceEvent" />
    <owl:Class rdf:ID="Employee" />
    <owl:Class rdf:ID="Customer" />
    <owl:Class rdf:ID="Equipment" />
    <owl:Class rdf:ID="TransportationMode" />
    <owl:Class rdf:ID="FinancialTransaction" />
    <owl:Class rdf:ID="ComplianceStandard" />
    <owl:Class rdf:ID="EnvironmentalStandard" />
    <owl:Class rdf:ID="SafetyStandard" />
    <owl:Class rdf:ID="SupplyChainEventType" />
    <owl:Class rdf:ID="RiskManagement" />
    <owl:Class rdf:ID="IncidentReport" />
    <owl:Class rdf:ID="Region" />
    <owl:Class rdf:ID="Route" />
    <owl:Class rdf:ID="ProductionBatch" />
    <owl:Class rdf:ID="QualityInspection" />
    <owl:Class rdf:ID="MaintenanceSchedule" />
    <owl:Class rdf:ID="Invoice" />
    <owl:Class rdf:ID="PaymentTerms" />

    <!-- Properties -->
    <owl:ObjectProperty rdf:ID="supplies">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="ProductLine" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="manufactures">
        <rdfs:domain rdf:Resource="Plant" />
        <rdfs:range rdf:Resource="ProductLine" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="stores">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="Product" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="distributes">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Product" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="partnersWith">
        <rdfs:domain rdf:Resource="Partner" />
        <rdfs:range rdf:Resource="Supplier" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="owns">
        <rdfs:domain rdf:Resource="Office" />
        <rdfs:range rdf:Resource="Plant" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="mapsTo">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="Warehouse" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="shipsFrom">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="Distributor" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="receivesFrom">
