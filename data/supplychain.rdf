    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="logisticsStrategyAttribute">
        <rdfs:domain rdf:Resource="Logistics" />
        <rdfs:range rdf:Resource="logisticsStrategy" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="qualityControlStandardAttribute">
        <rdfs:domain rdf:Resource="QualityControl" />
        <rdfs:range rdf:Resource="qualityControlStandard" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="safetyComplianceStandardAttribute">
        <rdfs:domain rdf:Resource="SafetyCompliance" />
        <rdfs:range rdf:Resource="safetyComplianceStandard" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="environmentalImpactScoreAttribute">
        <rdfs:domain rdf:Resource="EnvironmentalImpact" />
        <rdfs:range rdf:Resource="environmentalImpactScore" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="latAttribute">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="lat" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="longAttribute">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="long" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="latAttribute">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="lat" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="longAttribute">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="long" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="scheduleDateAttribute">
        <rdfs:domain rdf:Resource="ProductionSchedule" />
        <rdfs:range rdf:Resource="scheduleDate" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="machineIDAttribute">
        <rdfs:domain rdf:Resource="Machine" />
