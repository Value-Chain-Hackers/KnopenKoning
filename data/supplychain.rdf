        <rdfs:range rdf:Resource="Product" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="retailsThrough">
        <rdfs:domain rdf:Resource="Store" />
        <rdfs:range rdf:Resource="Product" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="transports">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Product" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="tracks">
        <rdfs:domain rdf:Resource="Office" />
        <rdfs:range rdf:Resource="SupplyChainEvent" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="uses">
        <rdfs:domain rdf:Resource="Plant" />
        <rdfs:range rdf:Resource="RawMaterial" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="hasInventory">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="Inventory" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="placesOrder">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Order" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="ships">
        <rdfs:domain rdf:Resource="Warehouse" />
        <rdfs:range rdf:Resource="Shipment" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="delivers">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Delivery" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="makesPayment">
        <rdfs:domain rdf:Resource="Partner" />
        <rdfs:range rdf:Resource="Payment" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="transportsVia">
        <rdfs:domain rdf:Resource="Distributor" />
        <rdfs:range rdf:Resource="Transportation" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="manages">
        <rdfs:domain rdf:Resource="Office" />
        <rdfs:range rdf:Resource="Logistics" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="implements">
        <rdfs:domain rdf:Resource="Plant" />
        <rdfs:range rdf:Resource="QualityControl" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="compliesWith">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="SafetyCompliance" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="hasImpactOn">
        <rdfs:domain rdf:Resource="ProductLine" />
        <rdfs:range rdf:Resource="EnvironmentalImpact" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="schedules">
        <rdfs:domain rdf:Resource="Plant" />
        <rdfs:range rdf:Resource="ProductionSchedule" />
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="operates">
