    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:ID="basedOn">
        <rdfs:domain rdf:Resource="Payment" />
        <rdfs:range rdf:Resource="PaymentTerms" />
    </owl:ObjectProperty>

    <!-- DataTypes -->
    <owl:Datatype rdf:ID="location" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="quantity" rdf:datatype="xsd:integer" />
    <owl:Datatype rdf:ID="supplyChainEventDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="materialType" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="orderDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="shipmentDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="deliveryDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="paymentDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="amount" rdf:datatype="xsd:decimal" />
    <owl:Datatype rdf:ID="transportationMode" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="logisticsStrategy" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="qualityControlStandard" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="safetyComplianceStandard" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="environmentalImpactScore" rdf:datatype="xsd:decimal" />
    <owl:Datatype rdf:ID="lat" rdf:datatype="xsd:decimal" />
    <owl:Datatype rdf:ID="long" rdf:datatype="xsd:decimal" />
    <owl:Datatype rdf:ID="scheduleDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="machineID" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="maintenanceDate" rdf:datatype="xsd:date" />
    <owl:Datatype rdf:ID="operationHours" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="capacity" rdf:datatype="xsd:integer" />
    <owl:Datatype rdf:ID="contactInfo" rdf:datatype="xsd:string" />
    <owl:Datatype rdf:ID="cost" rdf:datatype="xsd:decimal" />
    <owl:Datatype rdf:ID="performanceMetric" rdf:datatype="xsd:string" />

    <!-- Attributes -->
    <owl:DatatypeProperty rdf:ID="locationAttribute">
        <rdfs:domain rdf:Resource="Supplier" />
        <rdfs:range rdf:Resource="location" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="quantityAttribute">
        <rdfs:domain rdf:Resource="ProductLine" />
        <rdfs:range rdf:Resource="quantity" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="eventDateAttribute">
        <rdfs:domain rdf:Resource="SupplyChainEvent" />
        <rdfs:range rdf:Resource="supplyChainEventDate" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="materialTypeAttribute">
        <rdfs:domain rdf:Resource="RawMaterial" />
        <rdfs:range rdf:Resource="materialType" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="orderDateAttribute">
        <rdfs:domain rdf:Resource="Order" />
        <rdfs:range rdf:Resource="orderDate" />
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:ID="shipmentDateAttribute">
